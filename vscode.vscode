                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    "name": "otros3",
                    "scope": "storage.type.function.python",
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    "name": "otros3",
                    "scope": "keyword.operator.logical.python",
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
            ]
        }
